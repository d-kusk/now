{
    "extends": [
        "standard",
        "plugin:react/recommended",
        "plugin:import/errors",
        "plugin:import/warnings",
        "plugin:import/typescript",
        "prettier"
    ],
    "plugins": [
        "import",
        "react",
        "prettier"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "no-unused-vars": "off",
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "semi": false
            }
        ]
    }
}
